<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/17"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="Controllers.BackOfficeController"
      stylesheets="@/styles.css"
      alignment="CENTER"
      spacing="20"
      style="-fx-padding: 30;">

    <Label text="Administration Cultify"
           styleClass="title-label"/>

    <Button text="Gestion des Contenus"
            onAction="#openContenuView"
            styleClass="btn-primary"/>

    <Button text="Gestion des Quiz"
            onAction="#openQuizView"
            styleClass="btn-primary"/>

    <Button text="Gestion des Questions"
            onAction="#openQuestionView"
            styleClass="btn-primary"/>
</VBox>